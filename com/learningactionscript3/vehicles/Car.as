package com.learningactionscript3.vehicles {		import com.learningactionscript3.vehicles.Vehicle;	import com.learningactionscript3.accessories.Tires;	import com.learningactionscript3.accessories.soundsystems.CDPlayer;		public class Car extends Vehicle {				private var _tires:Tires;		public function Car(mpg:Number, fuel:Number) {			super(mpg, fuel);			_tires = new Tires("highperformance");			trace(this, "has", _tires.type, "tires");			soundSystem = new CDPlayer();		}				public function openSunroof () {			trace(this, "opened sunroof");		}				override public function useAccessory():void{			openSunroof();		}	}}