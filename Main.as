package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;	import com.learningactionscript3.vehicles.Car;	import com.learningactionscript3.vehicles.Truck;		public class Main extends MovieClip {				private var compact:Car;		private var pickup:Truck;		public function Main() {						compact = new Car(21, 18);			compact.x = 20;			compact.y = 20;			addChild(compact);			compact.changeGear();			compact.useAccessory();						pickup = new Truck(16, 23);			pickup.x = 20;			pickup.y = 100;			addChild(pickup);			pickup.changeGear();			pickup.useAccessory();						this.addEventListener(Event.ADDED_TO_STAGE, 								  onAddedToStage, false, 0, true);		}				private function onAddedToStage(evt:Event):void {			this.removeEventListener(Event.ADDED_TO_STAGE, 									 onAddedToStage);			stage.addEventListener(MouseEvent.CLICK, onClick, 								   false, 0, true);		}				private function onClick(evt:MouseEvent):void {			compact.go();			pickup.go();			compact.soundSystem.turnOn();			compact.soundSystem.playSelection(2);			pickup.soundSystem.turnOn();		}	}	}