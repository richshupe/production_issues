package  {		import com.learningactionscript3.vehicles.Car;	import com.learningactionscript3.vehicles.Truck;		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;		public class Main extends MovieClip {				private var _compact:Car;		private var _pickup:Truck;		public function Main() {						_compact = new Car(21, 18);			_compact.x = 20;			_compact.y = 20;			addChild(_compact);			_compact.changeGear();			_compact.useAccessory();						_pickup = new Truck(16, 23);			_pickup.x = 20;			_pickup.y = 100;			addChild(_pickup);			_pickup.changeGear();			_pickup.useAccessory();						this.addEventListener(Event.ADDED_TO_STAGE, 								  onAddedToStage, false, 0, true);		}				private function onAddedToStage(evt:Event):void {			this.removeEventListener(Event.ADDED_TO_STAGE, 									 onAddedToStage);			stage.addEventListener(MouseEvent.CLICK, onClick, 								   false, 0, true);		}				private function onClick(evt:MouseEvent):void {			_compact.go();			_pickup.go();			_compact.soundSystem.turnOn();			_compact.soundSystem.playSelection(2);			_pickup.soundSystem.turnOn();		}	}	}